{
  "name": "sql-match",
  "description": "Match a string using an SQL pattern.",
  "version": "1.0.4",
  "license": "MIT",
  "author": "Steven Vachon <contact@svachon.com> (https://www.svachon.com/)",
  "repository": "stevenvachon/sql-match",
  "dependencies": {
    "escape-string-regexp": "^1.0.5",
    "is-string": "^1.0.4"
  },
  "devDependencies": {
    "babelify": "^8.0.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "benchmark": "^2.1.4",
    "browserify": "^16.2.2",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "gzip-size-cli": "^3.0.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "puppeteer": "^1.6.2",
    "uglify-es": "^3.3.9"
  },
  "engines": {
    "node": ">= 6"
  },
  "scripts": {
    "benchmark": "node benchmark.js",
    "browserify": "browserify index.js --transform [ babelify --presets [ env ] ] --standalone=sqlMatch | uglifyjs --compress --mangle -o browser.js",
    "ci": "npm test && nyc report --reporter=text-lcov | coveralls",
    "posttest": " nyc report --reporter=html && npm run benchmark --silent && gzip-size browser.js",
    "pretest": "npm run browserify --silent",
    "test": "nyc --reporter=text-summary mocha test.js --check-leaks --bail"
  },
  "files": [
    "browser.js",
    "index.js"
  ],
  "keywords": [
    "match",
    "sql",
    "wildcard"
  ]
}
